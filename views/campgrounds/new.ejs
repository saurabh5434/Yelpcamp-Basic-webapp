 <%- include("../partials/header") %>
<div class="container">
	<div class="row">
		<div class="col -lg-12">
			<h1 class="title">Create New Campground</h1><br>
			<div class="camp-form">
				<form action="/campgrounds" method="POST">
					<div class="form-group">
						<input class="form-control" type="text" placeholder="name" name="name">
					</div>
					<div class="form-group">
						<input class="form-control" type="text" placeholder="image url" name="image">
					</div>
					<div class="form-group">
						<input class="form-control" type="text" placeholder="decription" name="decription">
					</div>
					<div class="form-group">
						<button class="btn btn-primary btn-lg btn-block">Submit</button>
					</div>
				</form>
				<a href="/campgrounds">Back</a>
			</div>
		</div>
		
	</div>
	
</div>


 <%- include("../partials/footer") %>